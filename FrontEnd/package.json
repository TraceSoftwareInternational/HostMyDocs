{
  "name": "host-my-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:build": "compodoc --name HostMyDocs --tsconfig tsconfig.json --theme readthedocs --toggleMenuItems all",
    "docs:watch": "compodoc --name HostMyDocs --tsconfig tsconfig.json --theme readthedocs --watch --serve --port 4000",
    "docs:coverage": "compodoc --name HostMyDocs --tsconfig tsconfig.json --coverageTest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@clr/angular": "^2.0.1",
    "@clr/icons": "^2.0.1",
    "@clr/ui": "^2.0.1",
    "@webcomponents/custom-elements": "^1.0.0",
    "clipboard": "^2.0.1",
    "core-js": "^3.1.4",
    "rxjs": "^6.2.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@compodoc/compodoc": "^1.1.3",
    "@types/clipboard": "^2.0.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.0.10",
    "codelyzer": "^5.1.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5"
  }
}
